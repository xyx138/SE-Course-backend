{
    "mcpServers": {
        "filesystem": {
            "command": "npx.cmd",
            "args": [
              "-y",
              "@modelcontextprotocol/server-filesystem",
              "${PROJECT_PATH}"
            ]
        },
        "UML-MCP-Server": {
            "command": "uv",
            "args": [
                "run",
                "${PROJECT_PATH}\\src\\utils\\uml_mcp_server.py"
            ]
        },
        "bingcn": {
            "command": "npx.cmd",
            "args": [
              "-y",
              "bing-cn-mcp"
            ]
        },
        "fetch": {
            "command": "uvx",
            "args": [ "mcp-server-fetch"]
        }
    }
}